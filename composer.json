{
    "name": "forrestedw/query-url-builder",
    "description": "A one class package to simplify manipulating front end query urls",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "forrestedw",
            "email": "forrestedw@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.2",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Forrestedw\\QueryUrlBuilder\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Forrestedw\\QueryUrlBuilder\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Forrestedw\\QueryUrlBuilder\\QueryUrlBuilderServiceProvider"
            ],
            "aliases": {
                "QueryUrl": "Forrestedw\\QueryUrlBuilder\\QueryUrl"
            }
        }
    }
}
